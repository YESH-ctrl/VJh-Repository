{
    "manifest_version": 3,
    "name": "Fact Checker",
    "version": "1.0",
    "description": "Check if a text is likely fake news.",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
